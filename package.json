    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0"
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "dev": "cross-env NODE_ENV=development NODE_APIENV=development ENV=dev webpack-dev-server --open --config ./webpack.config.js --color --progress",
    "test": "cross-env NODE_ENV=test NODE_APIENV=testing ENV=test webpack-dev-server --open --config ./webpack.config.js --color --progress",
    "prod": "cross-env NODE_ENV=prod NODE_APIENV=proding webpack-dev-server --open --mode development --config ./webpack.config.js --color --progress",
    "build": "webpack-cli --config ./webpack.config.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
